<?xml version="1.0" encoding="UTF-8"?>

<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
         http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.1.xsd">

    <changeSet id="2020-07-15-initial-shop_order-1" author="Alexey Andrianov">

        <createTable tableName="shop_order" >
            <column name="order_id" autoIncrement="true" type="serial" >
                <constraints primaryKey="true"/>
            </column>
            <column name="date" type="timestamptz" />
            <column name="courier" type="courier" />
            <column name="shop_user" type="shop_user" />
            <column name="shop" type="shop" />
            <column name="order_status" type="order_status" />
            <column name="delivery_method" type="delivery_method" />
        </createTable>
    </changeSet>

</databaseChangeLog>